@model IEnumerable<StoreApp.Models.Models.ReviewModel>
@if (Model != null && Model.Any())
{
    foreach (var review in Model)
    {
        <div>
            <p><strong>@review.ApplicationUser?.UserName</strong>: @review.Review</p>
            <p>Rating: @review.Rating/5</p>
        </div>
    }
}
else
{
    <p>No reviews yet. Be the first to write one!</p>
}